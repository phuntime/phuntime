{
    "name": "phuntime/phuntime",
    "description": "Run PHP Apps on FaaS platforms",
    "type": "library",
    "bin": ["bin/phuntime-local.php"],
    "autoload": {
        "psr-0": {
            "Phuntime\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-0": {
            "Phuntime\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "vimeo/psalm": "^3.11",
        "swoole/ide-helper": "@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "pizzaminded",
            "email": "mikolajczajkowsky@gmail.com"
        }
    ],
    "require": {
        "psr/log": "^1.1",
        "nyholm/psr7": "^1.3",
        "symfony/http-client-contracts": "^2.1",
        "symfony/http-client": "^5.1",
        "symfony/process": "^5.1"
    }
}
